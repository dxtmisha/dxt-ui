{
  "name": "@dxtmisha/d1",
  "private": false,
  "version": "0.1.5",
  "type": "module",
  "description": "DXT-UI component library",
  "keywords": [
    "vue",
    "vue3",
    "ui",
    "components",
    "dxt-ui"
  ],
  "author": "dxtmisha@gmail.com",
  "license": "MIT",
  "homepage": "https://github.com/dxtmisha/dxt-ui/tree/main/packages/d1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dxtmisha/dxt-ui.git",
    "directory": "packages/d1"
  },
  "bugs": {
    "url": "https://github.com/dxtmisha/dxt-ui/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "dxt-ui": "dxt-ui ripple",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist/",
    "src/styles",
    "README.md",
    "package.json",
    "LICENSE"
  ],
  "exports": {
    "./plugin": {
      "import": "./dist/plugin.js",
      "types": "./dist/library/plugin.d.ts"
    },
    "./style.css": "./dist/style.css",
    "./style/ui-properties.scss": "./src/styles/Ui/style.scss",
    "./style/ui.scss": "./src/styles/Ui/main.scss",
    "./types.d.ts": {
      "types": "./dist/library/types.d.ts"
    },
    "./D1Image": {
      "types": "./dist/D1Image.d.ts",
      "default": "./dist/D1Image.js"
    },
    "./D1Icon": {
      "types": "./dist/D1Icon.d.ts",
      "default": "./dist/D1Icon.js"
    },
    "./D1Ripple": {
      "types": "./dist/D1Ripple.d.ts",
      "default": "./dist/D1Ripple.js"
    }
  },
  "buildOptions": {
    "name": "@dxtmisha/constructor",
    "formats": [
      "esm-bundler"
    ]
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@dxtmisha/configuration": "*",
    "@dxtmisha/constructor": "*",
    "@dxtmisha/functional": "*",
    "@dxtmisha/functional-basic": "*",
    "@dxtmisha/media": "*",
    "@dxtmisha/styles": "*"
  },
  "sideEffects": false
}