{
  "main": "{d.field}",
  "padding": "{??height} * {?sys.spacing.adaptive.md}",
  "space": "{sys.spacing.x}",
  "height": "{ref.spacing.12}",
  "link|font": "{font.bodyLarge}",
  "color": "{sys.palette.onSurface}",
  "border-width": "{sys.spacing.outline}",
  "border-color": "{sys.palette.outline}",
  "border-opacity": "{sys.opacity.outline}",
  "border-radius": "{??height} * {?sys.rounded.adaptive.md}",
  "focusWithin": {
    "color": "{sys.palette.primary}",
    "color-opacity": "{sys.opacity.surface.high}",
    "border-color": "{sys.palette.primary}",
    "border-width": "{ref.spacing.2px}",
    "outline-color": "{sys.palette.primary}",
    "outline-opacity": "{sys.opacity.outline}",
    "outline-width": "{ref.spacing.1}",
    "caption": {
      "opacity": "{sys.opacity.surface.disabled}"
    }
  },
  "validation": {
    "color": "{sys.palette.error}",
    "border-color": "{sys.palette.error}"
  },
  "#body": {
    "background-color": "{sys.palette.surface}",
    "background-opacity": "{sys.opacity.primary.high}",
    "gradient": "{sys.palette.primary}",
    "gradient-opacity": "0",
    "backdrop-filter": "{sys.blur.md}",
    "#input": {
      "gradient": "{sys.palette.primary}",
      "gradient-opacity": "0",
      "hover": {
        "gradient-opacity": "{sys.opacity.hover}"
      },
      "active": {
        "gradient-opacity": "{sys.opacity.pressed}"
      },
      "::placeholder": {
        "color-opacity": "{sys.opacity.surface.medium}"
      }
    },
    "focus": {
      "gradient-opacity": "{sys.opacity.focus}"
    }
  },
  "#prefix": {
    "link|font": "{font.bodyMedium}",
    "color-opacity": "{sys.opacity.surface.medium}"
  },
  "#suffix": {
    "link|font": "{font.bodyMedium}",
    "color-opacity": "{sys.opacity.surface.medium}"
  },
  "#caption": {
    "link|font": "{font.bodyLarge}",
    "color-opacity": "{sys.opacity.surface.medium}"
  },
  "#trailing": {
    "color-opacity": "{sys.opacity.surface.medium}"
  },
  "disabled": {
    "color-opacity": "{sys.opacity.surface.disabled}",
    "border-opacity": "{sys.opacity.outline} * {sys.opacity.surface.disabled}",
    "#body": {
      "background-color": "{sys.palette.surface}",
      "#input": {
        "color": "inherit",
        "::placeholder": {
          "color": "inherit"
        }
      }
    },
    "#prefix": {
      "color": "inherit"
    },
    "#suffix": {
      "color": "inherit"
    },
    "#caption": {
      "color": "inherit"
    }
  },
  "selected": {
    "#body": {
      "gradient-opacity": "{sys.opacity.selected}"
    }
  },
  "~basic": {
    "padding": "{grid.paddingX}",
    "border-opacity": "0",
    "background-opacity": "0",
    "backdrop-filter": "unset",
    "disabled": {
      "border-opacity": "0"
    }
  },
  "~tonal": {
    "border-width": "0",
    "focusWithin": {
      "border-width": "0"
    },
    "#body": {
      "background-color": "{sys.palette.primary}",
      "background-opacity": "{sys.opacity.overlay}",
      "backdrop-filter": "{sys.blur.xl}"
    },
    "&.??--validation": {
      "#body": {
        "background-color": "{sys.palette.error}",
        "gradient": "{sys.palette.error}",
        "#input": {
          "gradient": "{sys.palette.error}"
        }
      }
    }
  },
  "~filled": {
    "#body": {
      "background-color": "{sys.palette.scrim}",
      "background-opacity": "{sys.opacity.overlay}",
      "backdrop-filter": "{sys.blur.xl}"
    }
  },
  "~outlined": {
    "#body": {
      "#input": {
        "hover": {
          "gradient-opacity": "0"
        },
        "active": {
          "gradient-opacity": "0"
        }
      },
      "focus": {
        "gradient-opacity": "0"
      }
    },
    "~selected": {
      "border-width": "{ref.spacing.2px}",
      "border-color": "{sys.palette.primaryVariant}",
      "border-opacity": "{sys.opacity.scrim}",
      "#body": {
        "gradient-opacity": "0"
      }
    }
  },
  "~classic": {
    "_default": true
  }
}
