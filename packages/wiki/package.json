{
  "name": "@dxtmisha/wiki",
  "private": false,
  "version": "0.23.3",
  "type": "module",
  "description": "Wiki documentation and storybook utilities for DXT UI design system",
  "keywords": [
    "wiki",
    "storybook",
    "documentation",
    "ui",
    "dxt",
    "design-system",
    "components",
    "utilities"
  ],
  "author": "dxtmisha@gmail.com",
  "license": "MIT",
  "homepage": "https://github.com/dxtmisha/dxt-ui/tree/main/packages/wiki",
  "repository": {
    "type": "git",
    "url": "https://github.com/dxtmisha/dxt-ui.git",
    "directory": "packages/wiki"
  },
  "bugs": {
    "url": "https://github.com/dxtmisha/dxt-ui/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "library": "dxt-library",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "src/styles",
    "README.md",
    "package.json",
    "LICENSE"
  ],
  "main": "dist/library.js",
  "module": "dist/library.js",
  "types": "dist/library.d.ts",
  "exports": {
    ".": {
      "import": "./dist/library.js",
      "types": "./dist/library.d.ts"
    },
    "./media": {
      "import": "./dist/media.js",
      "types": "./dist/media.d.ts"
    },
    "./storybook": {
      "import": "./dist/storybook.js",
      "types": "./dist/storybook.d.ts"
    },
    "./storybook/styles": "./src/styles/storybookStyle.scss",
    "./types/*": "./dist/*"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    }
  },
  "dependencies": {},
  "sideEffects": false
}
