{
  "main": "{d.button}",
  "margin-icon": "{st.global.distance.space.inset.xs}",
  "box-sizing": "content-box",
  "color": {
    "value": "{st.action.foreground.color.primary.normal}",
    "_rename": "palette-color"
  },
  "focus-visible": {
    "box-shadow": "{st.elevation.onLight.focus.md}"
  },
  "hover": {
    "color": "{st.action.foreground.color.primary.hover}"
  },
  "active": {
    "color": "{st.action.foreground.color.primary.active}"
  },
  "selected": {
    "color": "{st.action.foreground.color.primary.select}"
  },
  "disabled": {
    "opacity": "0.5"
  },
  "~primary": {
    "color": {
      "value": "{st.action.foreground.color.onprimary.normal}",
      "_rename": "color"
    },
    "background-color": {
      "value": "{st.action.background.color.primary.solid.normal}",
      "_rename": "palette-background"
    },
    "hover": {
      "color": "{st.action.foreground.color.onprimary.hover}",
      "background-color": "{st.action.background.color.primary.solid.hover}"
    },
    "active": {
      "color": "{st.action.foreground.color.onprimary.active}",
      "background-color": "{st.action.background.color.primary.solid.active}"
    },
    "selected": {
      "color": "{st.action.foreground.color.onprimary.select}",
      "background-color": "{st.action.background.color.primary.solid.select}"
    },
    "&.?palette--neutral": {
      "color": {
        "value": "{st.action.background.color.neutral.invert.normal}",
        "_rename": "color"
      },
      "background-color": {
        "value": "{st.action.background.color.neutral.solid.normal}",
        "_rename": "background-color"
      },
      "hover": {
        "color": "{st.action.background.color.neutral.invert.hover}",
        "background-color": "{st.action.background.color.neutral.solid.hover}"
      },
      "active": {
        "color": "{st.action.background.color.neutral.invert.active}",
        "background-color": "{st.action.background.color.neutral.solid.active}"
      },
      "selected": {
        "color": "{st.action.background.color.neutral.invert.select}",
        "background-color": "{st.action.background.color.neutral.solid.select}"
      }
    },
    "_category": "style",
    "_default": true
  },
  "~primaryInverse": {
    "color": {
      "value": "{st.content.foreground.color.neutral.primary}",
      "_rename": "color"
    },
    "background-color": {
      "value": "{st.action.background.color.neutral.invert.normal}",
      "_rename": "palette-background"
    },
    "hover": {
      "color": "{st.content.foreground.color.neutral.invert.primary}",
      "background-color": "{st.action.background.color.neutral.solid.hover}"
    },
    "active": {
      "color": "{st.content.foreground.color.neutral.invert.primary}",
      "background-color": "{st.action.background.color.neutral.solid.active}"
    },
    "selected": {
      "color": "{st.content.foreground.color.neutral.invert.primary}",
      "background-color": "{st.action.background.color.neutral.solid.select}"
    },
    "_category": "style"
  },
  "~secondary": {
    "background-color": {
      "value": "{st.action.background.color.primary.subtle.normal}",
      "_rename": "palette-background"
    },
    "hover": {
      "background-color": "{st.action.background.color.primary.subtle.hover}"
    },
    "active": {
      "background-color": "{st.action.background.color.primary.subtle.active}"
    },
    "selected": {
      "background-color": "{st.action.background.color.primary.subtle.select}"
    },
    "&.?palette--neutral": {
      "color": {
        "value": "{st.action.foreground.color.neutral.normal}",
        "_rename": "color"
      },
      "background-color": {
        "value": "{st.action.background.color.neutral.subtle.normal}",
        "_rename": "background-color"
      },
      "hover": {
        "color": "{st.action.foreground.color.neutral.hover}",
        "background-color": "{st.action.background.color.neutral.subtle.hover}"
      },
      "active": {
        "color": "{st.action.foreground.color.neutral.active}",
        "background-color": "{st.action.background.color.neutral.subtle.active}"
      },
      "selected": {
        "color": "{st.action.foreground.color.neutral.select}",
        "background-color": "{st.action.background.color.neutral.subtle.select}"
      }
    },
    "_category": "style"
  },
  "~outlined": {
    "color": "{st.action.foreground.color.neutral.normal}",
    "background-color": "{st.action.background.color.neutral.invert.normal}",
    "before": {
      "position": "absolute",
      "inset": "0px",
      "border-radius": "inherit",
      "border-style": "solid",
      "border-width": "1px",
      "border-color": "{st.action.border.color.neutral.subtle.normal}"
    },
    "hover": {
      "color": "{st.action.foreground.color.neutral.hover}",
      "background-color": "{st.action.background.color.neutral.invert.hover}",
      "border-color": "{st.action.border.color.neutral.subtle.hover}"
    },
    "active": {
      "color": "{st.action.foreground.color.neutral.active}",
      "background-color": "{st.action.background.color.neutral.invert.active}",
      "border-color": "{st.action.border.color.neutral.subtle.active}"
    },
    "selected": {
      "color": "{st.action.foreground.color.neutral.select}",
      "background-color": "{st.action.background.color.neutral.invert.select}",
      "border-color": "{st.action.border.color.neutral.subtle.select}"
    },
    "&.?palette": {
      "color": "{st.action.foreground.color.primary.normal}",
      "background-color": "{st.action.background.color.neutral.ghost.normal}",
      "before": {
        "border-color": {
          "value": "{st.action.foreground.color.primary.normal}",
          "_rename": "palette-border"
        }
      },
      "hover": {
        "color": "{st.action.foreground.color.primary.hover}",
        "background-color": "{st.action.background.color.neutral.ghost.normal}",
        "border-color": "{st.action.foreground.color.primary.hover}"
      },
      "active": {
        "color": "{st.action.foreground.color.primary.active}",
        "background-color": "{st.action.background.color.neutral.ghost.normal}",
        "border-color": "{st.action.foreground.color.primary.active}"
      },
      "selected": {
        "color": "{st.action.foreground.color.primary.selected}",
        "background-color": "{st.action.background.color.neutral.ghost.normal}",
        "border-color": "{st.action.foreground.color.primary.selected}"
      }
    },
    "&.?palette--neutral": {
      "color": {
        "value": "{st.action.foreground.color.neutral.normal}",
        "_rename": "color"
      },
      "before": {
        "border-color": {
          "value": "{st.action.foreground.color.neutral.normal}",
          "_rename": "border-color"
        }
      },
      "hover": {
        "color": "{st.action.foreground.color.neutral.hover}",
        "border-color": "{st.action.foreground.color.neutral.hover}"
      },
      "active": {
        "color": "{st.action.foreground.color.neutral.active}",
        "border-color": "{st.action.foreground.color.neutral.active}"
      },
      "selected": {
        "color": "{st.action.foreground.color.neutral.selected}",
        "border-color": "{st.action.foreground.color.neutral.selected}"
      }
    },
    "_category": "style"
  },
  "~ghost": {
    "background-color": "{st.action.background.color.neutral.ghost.normal}",
    "focus": {
      "background-color": "{st.action.background.color.neutral.ghost.focus}"
    },
    "hover": {
      "background-color": "{st.action.background.color.neutral.ghost.hover}"
    },
    "active": {
      "background-color": "{st.action.background.color.neutral.ghost.active}"
    },
    "selected": {
      "background-color": "{st.action.background.color.neutral.ghost.select}"
    },
    "&.?palette--neutral": {
      "color": {
        "value": "{st.action.foreground.color.neutral.normal}",
        "_rename": "color"
      },
      "hover": {
        "color": "{st.action.foreground.color.neutral.hover}"
      },
      "active": {
        "color": "{st.action.foreground.color.neutral.active}"
      },
      "selected": {
        "color": "{st.action.foreground.color.neutral.selected}"
      }
    },
    "_category": "style"
  },
  "~link": {
    "gap": "{st.global.distance.space.inset.xs}",
    "padding": {
      "value": "0",
      "_important": true
    },
    "hover": {
      "#label": {
        "text-decoration-line": "underline",
        "text-decoration-style": "dashed",
        "text-underline-offset": "{st.global.distance.space.inset.x}"
      }
    },
    "_category": "style"
  },
  "~adaptive": {
    "iconSm": {
      "media-max|sm": {
        "menuBasic": "{??adaptive.icon}"
      }
    },
    "iconMd": {
      "media-max|md": {
        "menuBasic": "{??adaptive.icon}"
      }
    },
    "iconLg": {
      "media-max|lg": {
        "menuBasic": "{??adaptive.icon}"
      }
    },
    "iconXl": {
      "media-max|xl": {
        "menuBasic": "{??adaptive.icon}"
      }
    },
    "fullSm": {
      "media|sm": {
        "menuBasic": "{??adaptive.icon}"
      }
    },
    "fullMd": {
      "media|md": {
        "menuBasic": "{??adaptive.icon}"
      }
    },
    "fullLg": {
      "media|lg": {
        "menuBasic": "{??adaptive.icon}"
      }
    },
    "fullXl": {
      "media|xl": {
        "menuBasic": "{??adaptive.icon}"
      }
    },
    "blockToAutoSm": {
      "media|sm": {
        "menuBasic": "{??adaptive.auto}"
      },
      "media-max|sm": {
        "menuBasic": "{??adaptive.block}"
      }
    },
    "blockToAutoMd": {
      "media|md": {
        "menuBasic": "{??adaptive.auto}"
      },
      "media-max|md": {
        "menuBasic": "{??adaptive.block}"
      }
    },
    "blockToAutoLg": {
      "media|lg": {
        "menuBasic": "{??adaptive.auto}"
      },
      "media-max|lg": {
        "menuBasic": "{??adaptive.block}"
      }
    }
  },
  "~size": {
    "x": {
      "padding": "{st.global.distance.space.inset.lg}",
      "height": "{ot.dimension.200}",
      "font": "{st.text.label.sm}",
      "border-radius": "{st.selectable.borderRadius.size.xs}"
    },
    "xs": {
      "padding": "{st.global.distance.space.inset.xl}",
      "height": "{ot.dimension.225}",
      "font": "{st.text.label.sm}",
      "border-radius": "{st.selectable.borderRadius.size.sm}"
    },
    "sm": {
      "padding": "{st.global.distance.space.inset.2xl}",
      "height": "{ot.dimension.250}",
      "font": "{st.text.label.sm}",
      "border-radius": "{st.selectable.borderRadius.size.sm}"
    },
    "md": {
      "padding": "{st.global.distance.space.inset.3xl} - {ot.dimension.12}",
      "height": "{ot.dimension.300} - {ot.dimension.25}",
      "font": "{st.text.label.md}",
      "border-radius": "{st.selectable.borderRadius.size.md}"
    },
    "lg": {
      "margin-icon": "{st.global.distance.space.inset.sm}",
      "padding": "{st.global.distance.space.inset.3xl}",
      "height": "{ot.dimension.300}",
      "font": "{st.text.label.md}",
      "border-radius": "{st.selectable.borderRadius.size.lg}"
    },
    "xl": {
      "margin-icon": "{st.global.distance.space.inset.md}",
      "padding": "{st.global.distance.space.inset.5xl}",
      "height": "{ot.dimension.350}",
      "font": "{st.text.label.lg}",
      "border-radius": "{st.selectable.borderRadius.size.xl}"
    },
    "autoMd": {
      "padding": "{st.global.distance.space.inset.xl}",
      "paddingY": "{st.global.distance.space.inset.md}",
      "height": "auto",
      "font": "{st.text.label.md}",
      "border-radius": "{st.selectable.borderRadius.size.md}",
      "#label": {
        "white-space": "wrap"
      }
    },
    "autoLg": {
      "padding": "{st.global.distance.space.inset.2xl}",
      "paddingY": "{st.global.distance.space.inset.lg}",
      "height": "auto",
      "font": "{st.text.label.lg}",
      "border-radius": "{st.selectable.borderRadius.size.md}",
      "#label": {
        "white-space": "wrap"
      }
    },
    "_default": "md"
  },
  "~rounded": {
    "border-radius": "{st.selectable.borderRadius.size.pill}"
  },
  "& &__label ~ .?icon": {
    "margin": {
      "value": "{??padding}",
      "_type": "var"
    }
  },
  "& &__label ~ &__icon": {
    "margin-right": "{??padding} * -1 + {??marginIcon}"
  },
  "& &__label ~ &__trailing": {
    "margin-left": "{??padding} * -1 + {??marginIcon}"
  },
  "@@palette": "{palette}"
}
