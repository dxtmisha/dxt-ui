  "type": "module",
  "description": "Core functional utility library for modern web development without framework dependencies",
  "keywords": [
    "functional",
    "utilities",
    "helpers",
    "typescript",
    "vue",
    "composables",
    "classes",
    "geo",
    "datetime",
    "cache",
