    "directory": "packages/functional"
  },
  "bugs": {
    "url": "https://github.com/dxtmisha/dxt-ui/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
