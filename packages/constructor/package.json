{
  "name": "@dxtmisha/constructor",
  "private": false,
  "version": "0.39.6",
  "type": "module",
  "description": "Vue component constructors for DXT UI design system - ready-to-use constructors with TypeScript support and SCSS styling",
  "keywords": [
    "constructor",
    "vue",
    "components",
    "typescript",
    "scss",
    "ui",
    "design-system",
    "dxt",
    "button",
    "field",
    "mask",
    "window",
    "list",
    "menu"
  ],
  "author": "dxtmisha@gmail.com",
  "license": "MIT",
