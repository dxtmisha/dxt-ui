{
  "menuSizeMultiplier": "1",
  "padding": {
    "_type": "var"
  },
  "width": {
    "value": {
      "auto": "auto",
      "max": "100%"
    },
    "_var": true,
    "_style": true
  },
  "height": {
    "value": {
      "auto": "auto",
      "max": "100%"
    },
    "_var": true,
    "_style": true
  },
  "imageSizeValue": {
    "_type": "var"
  },
  "iconPadding": {
    "_type": "var"
  },
  "background-color": {
    "_type": "var"
  },
  "background-opacity": {
    "_type": "var"
  },
  "transform": {
    "_type": "var"
  },
  "transform-origin": {
    "_type": "var"
  },
  "#body": {
    "#group": {},
    "#context": {},
    "_settingClone": true
  },
  "#control": {},
  "#image": {},
  "#close": {},
  "#teleport": {},
  "~hide": {},
  "~axis": {
    "x": {},
    "y": {},
    "on": {},
    "_default": "y"
  },
  "~imagePosition": {
    "~top": {},
    "~left": {},
    "_default": "top"
  },
  "~dense": {
    "padding": {
      "value": "0 !important",
      "_type": "property"
    },
    "#body": {
      "border-radius": "0"
    }
  },
  "~alignment": {
    "~center": {},
    "~top": {
      "align-items": "flex-start"
    },
    "~topRight": {
      "align-items": "flex-start",
      "justify-content": "flex-end"
    },
    "~topLeft": {
      "align-items": "flex-start",
      "justify-content": "flex-start"
    },
    "~topFull": {
      "align-items": "flex-start",
      "#body": {
        "width": "100%"
      }
    },
    "~right": {
      "justify-content": "flex-end"
    },
    "~rightFull": {
      "justify-content": "flex-end",
      "#body": {
        "top": "auto !important",
        "height": "100%"
      }
    },
    "~bottom": {
      "align-items": "flex-end"
    },
    "~bottomRight": {
      "align-items": "flex-end",
      "justify-content": "flex-end"
    },
    "~bottomLeft": {
      "align-items": "flex-end",
      "justify-content": "flex-start"
    },
    "~bottomFull": {
      "align-items": "flex-end",
      "#body": {
        "width": "100%"
      }
    },
    "~left": {
      "justify-content": "flex-start"
    },
    "~leftFull": {
      "justify-content": "flex-start",
      "#body": {
        "top": "auto !important",
        "height": "100%"
      }
    }
  },
  "~origin": {
    "~center": {
      "transform-origin": "center"
    },
    "~top": {
      "transform-origin": "top center"
    },
    "~right": {
      "transform-origin": "center \\#{ui.dirRight()}"
    },
    "~bottom": {
      "transform-origin": "bottom center"
    },
    "~left": {
      "transform-origin": "center \\#{ui.dirLeft()}"
    },
    "~topToBottom": {
      "transform-preparation": "translateY(-64px)",
      "transform-origin": "top center"
    },
    "~rightToLeft": {
      "transform-preparation": "translateX(calc(64px * \\#{ui.isDir()}))",
      "transform-origin": "center \\#{ui.dirRight()}"
    },
    "~bottomToTop": {
      "transform-preparation": "translateY(64px)",
      "transform-origin": "bottom center"
    },
    "~leftToRight": {
      "transform-preparation": "translateX(calc(-64px * \\#{ui.isDir()}))",
      "transform-origin": "center \\#{ui.dirLeft()}"
    }
  },
  "~adaptive": {
    "~menu": {
      "menuSizeMultiplier": "0.5",
      "content": "'--MENU--'",
      "position": "fixed",
      "top": "{??sys.inset.y}",
      "right": "auto",
      "bottom": "auto",
      "left": {
        "value": "{??sys.inset.x}",
        "_modification": false
      },
      "padding": {
        "value": "0",
        "_type": "property"
      },
      "height": "auto",
      "background-opacity": "0",
      "transform-preparation": "scale(.8)",
      "transform-origin": "{??origin.x} {??origin.y}",
      "body": {
        "top": "auto",
        "max-width": "100dvw - ({??padding} * 2)",
        "max-height": "100dvh - ({??padding} * 2)"
      },
      "#teleport": {
        "position": "static",
        "after": {
          "content": "none"
        }
      },
      "~widthMatch": {
        "#body": {
          "min-width": "{??sys.control.width}"
        }
      }
    },
    "~menuWindow": {
      "content": "'--MENU-WINDOW--'"
    },
    "~modal": {},
    "~modalDynamic": {
      "align-items": "flex-start",
      "body": {
        "position": "relative",
        "top": "{??sys.location}"
      }
    },
    "~static": {
      "content": "'--STATIC--'"
    }
  },
  "~fullscreen": {
    "link|dense": "{??dense}",
    "#body": {
      "width": "100%",
      "height": "100%",
      "max-width": "none",
      "max-height": "none"
    }
  },
  "~overscroll": {
    "_default": true
  },
  "~closeMobileHide": {},
  "~widthMatch": {}
}
