{
  "name": "@dxt-ui/media",
  "private": false,
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview"
  },
  "main": "dist/library.js",
  "module": "dist/library.js",
  "types": "dist/types/library.d.ts",
  "exports": {
    ".": {
      "import": [
        "./src/library.ts",
        "./dist/library.js"
      ],
      "types": "./dist/library.d.ts"
    },
    "./flags": {
      "import": [
        "./src/flags.ts",
        "./dist/flags.js"
      ],
      "types": "./dist/flags.d.ts"
    },
    "./types/*": "./dist/*",
    "./types/**/*.d.ts": "./dist/**/*.d.ts"
  },
  "buildOptions": {
    "name": "@dxt-ui/media",
    "formats": [
      "esm-bundler"
    ]
  },
  "dependencies": {},
  "devDependencies": {}
}
