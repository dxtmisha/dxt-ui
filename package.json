{
  "name": "dxt-ui",
  "private": false,
  "version": "0.1.5",
  "description": "A modular Vue 3 UI library and design system constructor featuring AI-automated documentation, TypeScript support, and a flexible component architecture.",
  "keywords": [
    "vue",
    "vue3",
    "ui",
    "ui-library",
    "component-library",
    "design-system",
    "typescript",
    "vite",
    "storybook",
    "frontend",
    "scss"
  ],
  "type": "module",
  "author": "dxtmisha@gmail.com",
  "license": "MIT",
  "homepage": "https://github.com/dxtmisha/dxt-ui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dxtmisha/dxt-ui.git"
  },
  "bugs": {
    "url": "https://github.com/dxtmisha/dxt-ui/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "package-library": "dxt-package library",
    "package-project": "dxt-package project",
    "build-functional": "dxt-build-functional",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "src/style.scss",
    "README.md",
    "package.json",
    "LICENSE"
  ],
  "exports": {
    "./functional": {
      "import": "./dist/functional.js",
      "types": "./dist/functional.d.ts"
    },
    "./media": {
      "import": "./dist/media.js",
      "types": "./dist/media.d.ts"
    },
    "./style.css": "./dist/style.css",
    "./scss": "./src/style.scss",
    "./types/*": "./dist/*"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@dxtmisha/configuration": "workspace:*",
    "@dxtmisha/constructor": "workspace:*",
    "@dxtmisha/functional": "workspace:*",
    "@dxtmisha/functional-basic": "workspace:*",
    "@dxtmisha/media": "workspace:*",
    "@dxtmisha/scripts": "workspace:*",
    "@dxtmisha/styles": "workspace:*",
    "@dxtmisha/wiki": "workspace:*",
    "sass": "^1.97.3",
    "typescript": "^5.9.3",
    "vue": "^3.5.27",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@dxtmisha/d1": "workspace:*",
    "@dxtmisha/storybook": "workspace:*",
    "@dxtmisha/test": "workspace:*",
    "@eslint/js": "^9.39.2",
    "@figma/plugin-typings": "^1.122.0",
    "@google/genai": "^1.38.0",
    "@mdx-js/rollup": "^3.1.1",
    "@napi-rs/canvas": "^0.1.88",
    "@storybook/addon-a11y": "^10.2.0",
    "@storybook/addon-docs": "^10.2.0",
    "@storybook/addon-onboarding": "^10.2.0",
    "@storybook/addon-vitest": "^10.2.0",
    "@storybook/vue3-vite": "^10.2.0",
    "@stylistic/eslint-plugin": "^5.7.1",
    "@types/node": "^25.0.10",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/browser": "^4.0.18",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "@volar/language-server": "^2.4.27",
    "@vue/language-server": "^3.2.4",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.23",
    "browserslist": "^4.28.1",
    "chromatic": "^13.3.5",
    "commander": "^14.0.2",
    "cssnano": "^7.1.2",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-plugin-storybook": "^10.2.0",
    "eslint-plugin-vue": "^10.7.0",
    "globals": "^17.1.0",
    "jsdom": "^27.4.0",
    "lightningcss": "^1.31.1",
    "magic-string": "^0.30.21",
    "playwright": "^1.58.0",
    "postcss": "^8.5.6",
    "postcss-discard-duplicates": "^7.0.2",
    "puppeteer": "^24.36.1",
    "storybook": "^10.2.0",
    "typescript-eslint": "^8.53.1",
    "vite": "^7.3.1",
    "vite-node": "^5.3.0",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-dts": "^4.5.4",
