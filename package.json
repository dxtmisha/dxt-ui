{
  "name": "dxt-ui",
  "private": false,
  "version": "0.1.2",
  "description": "A modular Vue 3 UI library and design system constructor featuring AI-automated documentation, TypeScript support, and a flexible component architecture.",
  "keywords": [
    "vue",
    "vue3",
    "ui",
    "ui-library",
    "component-library",
    "design-system",
    "typescript",
    "vite",
    "storybook",
    "frontend",
    "scss"
  ],
  "type": "module",
  "author": "dxtmisha@gmail.com",
  "license": "MIT",
  "homepage": "https://github.com/dxtmisha/dxt-ui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dxtmisha/dxt-ui.git"
  },
  "bugs": {
    "url": "https://github.com/dxtmisha/dxt-ui/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "package-library": "dxt-package library",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "README.md",
    "package.json",
    "LICENSE"
  ],
  "exports": {
    "./functional": {
      "import": "./dist/functional.js",
      "types": "./dist/functional.d.ts"
    },
    "./functional-basic": {
      "import": "./dist/functional-basic.js",
      "types": "./dist/functional-basic.d.ts"
    },
    "./media": {
      "import": "./dist/media.js",
      "types": "./dist/media.d.ts"
    },
    "./scripts": {
      "import": "./dist/scripts.js",
      "types": "./dist/scripts.d.ts"
    },
    "./types/*": "./dist/*"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@dxtmisha/configuration": "^0.4.2",
    "@dxtmisha/constructor": "0.30.1",
    "@dxtmisha/functional": "^1.6.4",
    "@dxtmisha/functional-basic": "^0.5.1",
    "@dxtmisha/media": "^0.4.2",
    "@dxtmisha/scripts": "^0.5.2",
    "@dxtmisha/styles": "^1.0.6",
    "@dxtmisha/wiki": "^0.25.1",
    "sass": "^1.97.2",
    "typescript": "^5.9.3",
    "vite-plugin-singlefile": "^2.3.0",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@dxt-ui/c2": "workspace:*",
    "@dxt-ui/test": "workspace:*",
    "@dxtmisha/d1": "workspace:*",
    "@dxtmisha/storybook": "workspace:*",
    "@eslint/js": "^9.39.2",
    "@figma/plugin-typings": "^1.121.0",
    "@google/genai": "^1.34.0",
    "@mdx-js/rollup": "^3.1.1",
    "@storybook/addon-a11y": "^10.1.11",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-onboarding": "^10.1.11",
    "@storybook/addon-vitest": "^10.1.11",
    "@storybook/vue3-vite": "^10.1.11",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@types/node": "^25.0.3",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/browser": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16",
    "@volar/language-server": "^2.4.27",
    "@vue/language-server": "^3.2.2",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.23",
    "chromatic": "^13.3.5",
    "commander": "^14.0.2",
    "cssnano": "^7.1.2",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-plugin-storybook": "^10.1.11",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^17.0.0",
    "jsdom": "^27.4.0",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "postcss-discard-duplicates": "^7.0.2",
    "storybook": "^10.1.11",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "vite": "^7.3.1",
    "vite-node": "^5.2.0",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-svg-loader": "^1.0.0",
    "vitest": "^4.0.16",
    "vue-tsc": "^3.2.2"
  }
}
